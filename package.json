{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "stripe:print-secret": "stripe listen --forward-to localhost:3000/api/stripe/webhook --print-secret",
    "stripe:listen:bg": "bash -lc 'mkdir -p .stripe && stripe listen --forward-to localhost:3000/api/stripe/webhook >.stripe/listen.log 2>&1 & echo $! > .stripe/listen.pid && echo Listener PID: $(cat .stripe/listen.pid)'",
    "stripe:stop": "bash -lc 'if [ -f .stripe/listen.pid ]; then kill $(cat .stripe/listen.pid) 2>/dev/null || true; rm -f .stripe/listen.pid; else pkill -f \"stripe listen\" 2>/dev/null || true; fi'",
    "dev:with-webhook": "bash -lc 'export STRIPE_WEBHOOK_SECRET=$(npm run -s stripe:print-secret) && npm run stripe:listen:bg && echo Using STRIPE_WEBHOOK_SECRET=$STRIPE_WEBHOOK_SECRET && next dev'",
    "trigger:checkout-completed": "stripe trigger checkout.session.completed",
    "trigger:pi-succeeded": "stripe trigger payment_intent.succeeded"
  },
